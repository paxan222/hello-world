QMenu
{
    color: #282828;
    font: "%FONT_FAMILY%";
    font-weight: %FONT_WEIGHT%;
    font-size: 15dip;
    border-style: solid;
	border-width: 1dip;
	border-color: #d4d4d4;
    background-color: #f2f2f2;
}
QMenu::icon
{
    padding-left: 8dip;
    padding-right: 8dip;
}
QMenu#statusMenu_::item
{
    padding-left: 35dip;
    padding-right: 16dip;
    height: 40dip;
}
QMenu#optionsMenu_::item
{
    padding-left: 16dip;
    padding-right: 16dip;
    height: 40dip;
}
QMenu::item:selected
{
    color: #282828;
    background-color: #dcdcdc;
}
QMenu::separator
{
    background-color: transparent;
}

QPushButton#statusButton_[self="true"]
{
    border: none;
    padding-left: 16dip;
    min-width: 48dip;
    min-height: 40dip;
}
QPushButton#statusButton_[self="false"]
{
    border: none;
    padding-left: 0dip;
    min-width: 48dip;
    min-height: 40dip;
}
QPushButton#statusButton_:pressed[self="true"]
{
    background-color: #66cae6b3;
}
QPushButton#statusButton_:pressed[self="false"]
{
    background-color: transparent;
}
QPushButton#statusButton_::menu-indicator
{
    image: url(:/resources/widgets/content_dropdown_black_open_100.png);
    width: 12dip;
    height: 8dip;
    subcontrol-origin: padding;
    subcontrol-position: center left;
    left: 10dip;
}

